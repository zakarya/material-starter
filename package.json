{
  "name": "material-starter",
  "version": "0.1.0",
  "description": "a starter for creating rich component libraries using angular-material",
  "scripts": {
    "watch": "gulp watch site --dev",
    "prom": "git pull --rebase origin master",
    "current-branch": "git rev-parse --abbrev-ref HEAD",
    "merge-base": "git merge-base $(npm run -s current-branch) origin/master",
    "squash": "git rebase -i $(npm run -s merge-base)"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zakarya/material-starter.git"
  },
